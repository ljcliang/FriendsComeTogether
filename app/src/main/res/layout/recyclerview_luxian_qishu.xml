<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="160dp">
        <RelativeLayout
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_marginTop="18dp"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_marginRight="2dp"
                    android:text="*"
                    android:textSize="16sp"
                    android:textColor="@color/red_d84c37"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:text="行程售价"
                    android:textColor="@color/black_333333"
                    android:textSize="16sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <EditText
                    android:inputType="numberDecimal"
                    android:paddingRight="5dp"
                    android:lines="1"
                    android:id="@+id/edt_input_price"
                    android:paddingLeft="3dp"
                    android:text="0"
                    android:gravity="end"
                    android:textSize="14sp"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/black_101010"
                    android:textColorHint="@color/black_666666"
                    android:background="@drawable/bg_d8d8d8_0px"
                    android:layout_width="68dp"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:layout_marginLeft="10dp"
                    android:text="元"
                    android:textColor="@color/black_333333"
                    android:textSize="16sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <RelativeLayout
                android:id="@+id/rl_btn_minus"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginLeft="10dp"
                    android:src="@mipmap/minus"
                    android:layout_width="22dp"
                    android:layout_height="22dp"/>
            </RelativeLayout>
        </RelativeLayout>
        <LinearLayout
            android:layout_marginLeft="12dp"
            android:layout_marginRight="41dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginRight="2dp"
                android:text="*"
                android:textSize="16sp"
                android:textColor="@color/red_d84c37"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:text="分销提成"
                android:textColor="@color/black_333333"
                android:textSize="16sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <EditText
                android:paddingRight="5dp"
                android:lines="1"
                android:id="@+id/edt_input_fenxiaoticheng"
                android:paddingLeft="3dp"
                android:text="0"
                android:gravity="end"
                android:textSize="14sp"
                android:layout_marginLeft="10dp"
                android:textColor="@color/black_101010"
                android:textColorHint="@color/black_666666"
                android:background="@drawable/bg_d8d8d8_0px"
                android:layout_width="68dp"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_marginLeft="10dp"
                android:text="元"
                android:textColor="@color/black_333333"
                android:textSize="16sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/iv_tishi_fenxiaoticheng"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="1dp"
                android:src="@mipmap/tishi"
                android:layout_width="20dp"
                android:layout_height="20dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_marginLeft="12dp"
            android:layout_marginRight="41dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginRight="2dp"
                android:text="*"
                android:textSize="16sp"
                android:textColor="@color/red_d84c37"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:text="发团时间"
                android:textColor="@color/black_333333"
                android:textSize="16sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:paddingRight="5dp"
                android:lines="1"
                android:id="@+id/tv_input_fatuanshijian_start"
                android:paddingLeft="3dp"
                android:text="0"
                android:gravity="end"
                android:textSize="14sp"
                android:layout_marginLeft="10dp"
                android:textColor="@color/black_101010"
                android:textColorHint="@color/black_666666"
                android:background="@drawable/bg_d8d8d8_0px"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:textSize="16sp"
                android:textColor="@color/black_333333"
                android:text="-"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:paddingRight="5dp"
                android:lines="1"
                android:id="@+id/tv_input_fatuanshijian_end"
                android:paddingLeft="3dp"
                android:text="0"
                android:gravity="end"
                android:textSize="14sp"
                android:textColor="@color/black_101010"
                android:textColorHint="@color/black_666666"
                android:background="@drawable/bg_d8d8d8_0px"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_marginLeft="12dp"
            android:layout_marginRight="41dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginRight="2dp"
                android:text="*"
                android:textSize="16sp"
                android:textColor="@color/red_d84c37"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:text="报名截止"
                android:textColor="@color/black_333333"
                android:textSize="16sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:paddingRight="5dp"
                android:lines="1"
                android:id="@+id/tv_input_baomingjiezhi"
                android:paddingLeft="3dp"
                android:text="0"
                android:gravity="end"
                android:textSize="14sp"
                android:layout_marginLeft="10dp"
                android:textColor="@color/black_101010"
                android:textColorHint="@color/black_666666"
                android:background="@drawable/bg_d8d8d8_0px"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
            <LinearLayout
                android:id="@+id/ll_woshi_daidui"
                android:layout_marginLeft="21dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/iv_woshi_daidui_check"
                    android:src="@mipmap/checkbox_black_true"
                    android:layout_width="20dp"
                    android:layout_height="20dp"/>
                <TextView
                    android:layout_marginLeft="6dp"
                    android:text="我是带队"
                    android:textColor="@color/black_101010"
                    android:textSize="14sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <View
        android:background="@color/color_d8d8d8"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
</LinearLayout>
